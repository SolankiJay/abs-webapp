<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.1.xsd">
    
    <grid name="appeal-grid" model="com.axelor.apps.fnath.db.Appeal" title="Types de recours" >
			<field name="name" />
    </grid>
    
    <form name="appeal-form" model="com.axelor.apps.fnath.db.Appeal" title="Type de recours" >
    	<panel>
    		<field name="name" />
    	</panel>
    </form>
    
    <grid name="appeal-object-grid" model="com.axelor.apps.fnath.db.AppealObject" title="Objets de recours" >
    	<field name="name" />
    </grid>
    
    <form name="appeal-object-form" model="com.axelor.apps.fnath.db.AppealObject" title="Objet de recours" >
    	<panel>
    		<field name="name" />
    	</panel>
    </form>
    
</object-views>
