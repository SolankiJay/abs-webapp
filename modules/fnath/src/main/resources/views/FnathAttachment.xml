<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.1.xsd">
    
    <grid name="fnath-attachment-grid" model="com.axelor.apps.fnath.db.FnathAttachment" title="Pièces jointes" >
    	<field name="name" />
    	<field name="metaFile" />
    </grid>
    
    <form name="fnath-attachment-form" model="com.axelor.apps.fnath.db.FnathAttachment" title="Pièce jointe" >
    	<panel>
    		<field name="name" />
    		<field name="metaFile" widget="binary-link" />
    	</panel>
    </form>
    
</object-views>
