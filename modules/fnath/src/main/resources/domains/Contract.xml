	<module name="contract" package="com.axelor.apps.contract.db" />

	<entity name="Contract" repository="none">
		
		
	</entity>

	<entity name="ContractVersion" repository="none">
		<integer name="contractManufacturingStatusSelect" title="État de fabrication de la carte" selection="contract.manufacturing.status.select" default="1"/>
